<div id="<%= dom_id materiel %>">
  <th scope="row">
    <%= materiel.id %>
  </th>
  <td>
    <%= materiel.mat_type %>
  </td>
  <td>
    <%= materiel.nom %>
  </td>
  <td>
    <%= materiel.created_at.strftime("%Y-%m-%d %H:%M") %>
  </td>
  <td>
    <%= button_to "", materiel, class:"btn btn-outline-primary bi bi-eye", :method => "get" %>
  </td>
</tr>


</div>
